<h3>ApplicationUserApplicationRole</h3>
<LoadingIndicator IsLoading="IsLoading"></LoadingIndicator>
<EditForm Model="@this.Model" OnValidSubmit="OnValidSubmitAsync">
<div class="mb-3">
<DataAnnotationsValidator></DataAnnotationsValidator>
<ValidationSummary></ValidationSummary>
</div>
<div class="mb-3">
<label class="form-label">@nameof(Model.ApplicationUserId)</label>
<InputNumber class="form-control" @bind-Value="Model.ApplicationUserId"/>
</div>
<div class="mb-3">
<label class="form-label">@nameof(Model.ApplicationRoleId)</label>
<InputNumber class="form-control" @bind-Value="Model.ApplicationRoleId"/>
</div>
<div class="mb-3">
<label class="form-label">@nameof(Model.ApplicationUserId)</label>
<InputSelect class="form-control" @bind-Value="Model.ApplicationUserId"></InputSelect>
</div>
<div class="mb-3">
<label class="form-label">@nameof(Model.ApplicationRoleId)</label>
<InputSelect class="form-control" @bind-Value="Model.ApplicationRoleId"></InputSelect>
</div>
<div class="mb-3">
<button class="btn btn-primary" type="submit">Submit</button>
</div>
</EditForm>
