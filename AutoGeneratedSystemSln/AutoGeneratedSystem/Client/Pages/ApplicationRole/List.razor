@page "/ApplicationRole/List"
<h3>ApplicationRole</h3>
<LoadingIndicator IsLoading="IsLoading"></LoadingIndicator>
@if (this.AllApplicationRole != null)
{
foreach (var singleApplicationRole in this.AllApplicationRole)
{
<ul class="list-group">
<li class="list-group-item">
@nameof(singleApplicationRole.ApplicationRoleId):@singleApplicationRole.ApplicationRoleId
@nameof(singleApplicationRole.RoleName):@singleApplicationRole.RoleName
@nameof(singleApplicationRole.RoleDescription):@singleApplicationRole.RoleDescription
</li>
</ul>
}
}
