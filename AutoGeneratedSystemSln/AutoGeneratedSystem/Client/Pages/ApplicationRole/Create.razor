@page "/ApplicationRole/Create"
<h3>ApplicationRole</h3>
<EditForm Model="@this.Model" OnValidSubmit="OnValidSubmitAsync">
<div class="mb-3">
<DataAnnotationsValidator></DataAnnotationsValidator>
<ValidationSummary></ValidationSummary>
</div>
<div class="mb-3">
<label class="form-label">@nameof(Model.RoleName)</label>
<InputText class="form-control" @bind-Value="Model.RoleName"/>
</div>
<div class="mb-3">
<label class="form-label">@nameof(Model.RoleDescription)</label>
<InputText class="form-control" @bind-Value="Model.RoleDescription"/>
</div>
<div class="mb-3">
<button class="btn btn-primary" type="submit">Submit</button>
</div>
</EditForm>
